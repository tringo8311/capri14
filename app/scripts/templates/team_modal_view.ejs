<div class="modal-dialog" data-easeout="rollIn">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title"><%= full_name %> / <%= title %></h4>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-5">
                    <div class="carousel">
                        <img alt="Member 1" src="<%= thumb %>"/>
                    </div>
                </div>
                <div class="col-md-7" style="height: 100%;">
                    <p><strong>Email</strong>: <%= email %></p>
                    <p><strong>Contact</strong>: <%= phone %></p>
                    <p><strong>Website</strong>: <%= url %></p>
                    <p><strong>Skills</strong>:
                    <div class="">
                        <% skills.split(",").map(function(label) {%>
                            <% percent = _(label).strRight(":") %>
                            <div class="progress">
                                <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= percent %>%;">
                                    <span class="sr-only"><%= percent %>%</span>
                                </div>
                                <span class="progress-type"><%= _(label).strLeft(":")%></span>
                                <span class="progress-completed"><%= percent %>%</span>
                            </div>
                        <%}); %>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="content">
                <p><%= content %></p>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>